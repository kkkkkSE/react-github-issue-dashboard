{
  "compilerOptions": {
    /* Language and Environment */
    "target": "es2021",
    "jsx": "react-jsx",

    /* Modules */
    "module": "commonjs",               
     "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,

    /* Completeness */
    "skipLibCheck": true
  }
}
